    "def r_name(data, regex1, regex2):\n",
    "    try:\n",
    "        d= re.search(regex1, str(data))\n",
    "        d= d.group()\n",
    "        print(d)\n",
    "        d= re.search (regex2, str(d))\n",
    "        d= d.group()\n",
    "        d= d.strip()\n",
    "        \n",
    "        return d\n",
    "    except:\n",
    "        return False\n",
    "\n"
