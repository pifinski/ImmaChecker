    "#Funktion um die PDF-Dateien einzulesen\n",
    "def parsing_pdf(filename):\n",
    "    try:\n",
    "        # opening pdf file\n",
    "        parsed_pdf = parser.from_file(filename)\n",
    "        data = parsed_pdf['content'] \n",
    "        \n",
    "        if data == None:\n",
    "            return False\n",
    "        \n",
    "        data= data.replace('\\n', '@')\n",
    "        data= data.replace(' ', '@')\n",
    "        data= re.sub(r'@{2,}', '@', data)\n",
    "        #print(data)\n",
    "        return data\n",
    "    except:\n",
    "        return False\n"
   ]
